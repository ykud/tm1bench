#region Prolog

# A wrapper process to execute tests

#Region Data load

logoutput('info','Load data single thread');

ExecuteProcess( 'tm1bench Test sales data load',
	'pNumSalesCells', pNumSalesCells,
	'pDisableLogging', 'Y' );

#EndRegion

#Region Reading data
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'All',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'All',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'Random',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
#EndRegion

#Region data load in parallel
RunProcess( 'tm1bench Test sales data load parallel',
	'pNumSalesCells', pNumSalesCells,
	'pNumberOfThreads', 2,
	'pDisableLogging', 'Y' );


#EndRegion
#endregion