#region Prolog

# A wrapper process to execute tests

#Region Data load


logoutput('info','Load data sequentially');
ExecuteProcess( 'tm1bench Test sales data load',
	'pNumSalesCells', pNumSalesCells,
	'pNumberOfThreads', 1,
	'pDisableLogging', 'Y' );

logoutput('info','Load data in 2 threads, expect close to 2x speed up');
ExecuteProcess( 'tm1bench Test sales data load',
	'pNumSalesCells', pNumSalesCells,
	'pNumberOfThreads', 2,
	'pDisableLogging', 'Y' );


#EndRegion

#Region Reading data
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'All',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'All',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
ExecuteProcess( 'tm1bench Test sales data read',
	'pProduct', 'Random',
	'pCustomer', 'All',
	'pResetCache', 'Y' );
#EndRegion
#endregion